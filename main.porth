main :: proc 
    "Hello, World!" out
end

helper :: proc
    func :: ... drop
    counter swap =
    iter swap =
    counter! iter! != if
        func()
        iter iter! 1 + =
        |func| iter! counter! helper()
    end
end

for :: macro
    func :: ... drop
    counter swap =
    |func| 0 counter! helper()
end

|main| 5 for